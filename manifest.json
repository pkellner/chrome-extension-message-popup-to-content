{
  "name": "message-popup-to-content",
  "version": "0.0.0.1",
  "description": "---",
  "author": "Peter Kellner",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["contentSettings", "tabs","activeTab",  "storage"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
